import csv
import sys

mydict = {}

with open('insurance.csv', mode='r') as infile:
    reader = csv.reader(infile)
    with open('insurance 2.csv', mode='w') as outfile:
        writer = csv.writer(outfile)
        mydict = {rows[0]:rows[1] for rows in reader}
       
 print(mydict)


import csv
import sys

mydict = {}

with open('insurance.csv', mode='r') as infile:
    reader = csv.DictReader(infile)
    for row in reader:
        mydict[row[0]] = row
        
       
print(mydict)
